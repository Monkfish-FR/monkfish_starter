{
    "name": "monkfish-fr/monkfish_starter",
    "description": "Basic starter kit for Drupal 8.",
    "type": "drupal-profile",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Fabien TAVERNIER",
            "email": "contact@monkfish.fr",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^1.22",
        "drupal/core": "~8.4",
        "drupal/devel": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/responsive_preview": "^1.0@alpha",
        "drupal/twig_extensions": "^1.0",
        "drupal/twig_field_value": "^1.1",
        "drupal/twig_tweak": "^2.0@alpha",
        "monkfish-fr/monkfish_adminimal": "dev-master",
        "monkfish-fr/monkfish_slick": "dev-master"
    },
    "extra": {
        "patches": {
            "drupal/twig_extensions": {
                "Fix Twig extensions trans bug": "patches/error_after_upgrading-2726531-9.patch"
            }
        }
    }
}
